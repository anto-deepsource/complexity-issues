{
  "compilerOptions": {
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "downlevelIteration": true,
    "outDir": "build",
    "esModuleInterop": true,
    "module": "ES2015",
    "moduleResolution": "bundler"
  },
  "include": [
    "src/"
  ],
  "exclude": [
    "build"
  ]
}